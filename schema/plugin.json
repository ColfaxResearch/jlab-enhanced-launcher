{
  "title": "Launcher",
  "description": "Launcher settings.",
  "jupyter.lab.shortcuts": [
    {
      "command": "launcher:create",
      "keys": [""],
      "selector": "body"
    }
  ],
  "properties": {
    "categories": {
      "title": "Launcher categories in displayed order",
      "description": "Absent categories will be appended.",
      "default": [
        "Kernels",
        "Featured AI Developer Tools",
        "Featured Tutorials",
        "Featured Sample Applications",
        "Terminal"
      ],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "customKernelSectionName": {
      "title": "Define the new name for Kernel Section ",
      "description": "Kernel section includes all the kernels as a default, just replacing the kernel section name",
      "default": "New Notebook & Terminal",
      "type": "string"
    },
    "combineKernelAndTerminal": {
      "title": "Define only if we want terminal into kernel section",
      "description": "By default terminal is in Terminal category but move it to kernel section",
      "default": "False",
      "type": "string"
    },
    "maxUsageAge": {
      "title": "Maximal age of the most recent use of a card (in days)",
      "description": "If the latest use of a card is higher than maxUsageAge, the card will be considered as never used before.",
      "default": 30,
      "type": "number"
    },
    "nRecentCards": {
      "title": "Number of recent cards to display",
      "default": 4,
      "type": "number"
    },
    "persistenMostUsedSection": {
      "title": "To keep Most Used section in Launcher",
      "default": "True",
      "type": "string"
    },
    "Featured AI Developer Tools": {
