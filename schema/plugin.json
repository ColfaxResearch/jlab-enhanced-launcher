{
  "title": "Launcher",
  "description": "Launcher settings.",
  "jupyter.lab.shortcuts": [
    {
      "command": "launcher:create",
      "keys": [""],
      "selector": "body"
    }
  ],
  "properties": {
    "categories": {
      "title": "Launcher categories in displayed order",
      "description": "Absent categories will be appended.",
      "default": [
        "Featured AI Developer Tools",
        "Featured Tutorials",
        "Featured Sample Applications",
        "Kernels"
      ],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "customKernelsCategoryName": {
      "title": "Define the new name for Kernels Section ",
      "description": "Make sure 'Kernels' is defined within categories property. Kernels section includes all the kernels as a default, just replacing the kernel section name.",
      "default": "New Notebooks",
      "type": "string"
    },
    "maxUsageAge": {
      "title": "Maximal age of the most recent use of a card (in days)",
      "description": "If the latest use of a card is higher than maxUsageAge, the card will be considered as never used before.",
      "default": 30,
      "type": "number"
    },
    "nRecentCards": {
      "title": "Number of recent cards to display",
      "default": 4,
      "type": "number"
    },
    "persistentMostUsedSection": {
      "title": "To keep Most Used section in Launcher",
      "default": false,
      "type": "boolean"
    },
